      "config": {},
      "process": null,
      "publish": [
        {
          "plugin_name": "influxdb",
          "config": {
            "host": "10.180.10.180",
            "port": 8086,
            "database": "snap",
            "user": "admin",
            "password": "admin"
          }
        }
      ]
